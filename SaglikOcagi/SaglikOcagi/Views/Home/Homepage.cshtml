@using SaglikOcagi.Entity
@model  IEnumerable<tbl_Anasayfa>

@{
    ViewBag.Title = "Homepage";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}


@foreach (var anasayfa in Model)
{
    <img class="col-lg-12" src="~/Images/@anasayfa.AnasayfaFoto" />
    <p class="col-lg-12" style="padding-top:20px">
        @anasayfa.Aciklama
    </p>
}



